<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <!-- 19/02/2015 (felix) Crear form para registro de aduanas -->
        <record id="group_aduana_admin" model="res.groups">
            <field name="name">Administrar aduanas</field>
            <field name="comment">El usuario puede administrar registros de aduana.</field>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
        
        <record id="group_aduana_user" model="res.groups">
            <field name="name">Ver aduanas</field>
            <field name="comment">El usuario puede ver registros de aduana.</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        
    </data>
</openerp>
